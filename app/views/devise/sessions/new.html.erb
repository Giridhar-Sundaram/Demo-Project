<h3>Welcome back </h3>
<br/>
<br/>
<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "login-page--form" }) do |f| %>
  <div class="input-field">
    <%= f.label :email,  class: "input-field__label"%>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="input-field">
    <%= f.label :password, class: "input-field__label"%>
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="input-field__checkbox">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me ,class: "input-field__label" %>
    </div>
  <% end %>
  <br/>
  <br/>
  <br/>
  <br/>
  <br/>
  <br/>
  <%= f.submit "Log in" , class: "btn--primary btn--primary--lg" %>
<% end %>
<br/>
<%= link_to "Sign up", new_registration_path(resource_name), class: "btn--secondary btn--secondary--lg" %>
